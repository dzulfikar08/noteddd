{
  "name": "notes-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "pages:build": "bunx @cloudflare/next-on-pages",
      "pages:dev": "bun pages:build && bun wrangler pages dev --port=3000",
      "preview": "bun pages:build && wrangler pages dev",
      "deploy": "bun pages:build && wrangler pages deploy",
      "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
      "db:generate": "drizzle-kit generate",
      "db:seed:local": "tsx seed/seed.ts && wrangler d1 execute notes-app --local --file seed/seed.sql",
      "db:seed-auth:local": "tsx seed/seed-auth.ts && wrangler d1 execute notes-app --local --file seed/seed-auth.sql",
      "db:seed:prod": "tsx seed/seed.ts && wrangler d1 execute hnotes-app --remote --file seed/seed.sql",
      "db:migrate:local": "wrangler d1 migrations apply notes-app --local",
      "db:migrate:preview": "wrangler d1 migrations apply --env preview notes-app-preview --remote",
      "db:migrate:prod": "wrangler d1 migrations apply notes-app --remote",
      "db:studio:local": "cross-env DB_LOCAL_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
      "db:studio:preview": "cross-env NODE_ENV='preview' drizzle-kit studio",
      "db:studio:prod": "drizzle-kit studio"
    },
